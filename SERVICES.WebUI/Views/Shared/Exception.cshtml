
@{
	Layout = null;
}

<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<title>Services 2 - Error 500</title>
	<!-- Tell the browser to be responsive to screen width -->
	<meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport"/>
	<!-- Bootstrap 3.3.6 -->
	<link rel="stylesheet" href="../../Plugins/bootstrap/css/bootstrap.min.css"/>
	<!-- Font Awesome -->
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css"/>
	<!-- Ionicons -->
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css"/>
	<!-- Theme style -->
	<link rel="stylesheet" href="../../Plugins/theme/AdminLTE.css"/>
	<link rel="stylesheet" href="../../Plugins//theme/custom.css"/>
  <!-- AdminLTE Skins. Choose a skin from the css/skins
  folder instead of downloading all of them to reduce the load. -->
  <link rel="stylesheet" href="../../Plugins/theme/skin-blue.css"/>

  <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
  <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
  <!--[if lt IE 9]>
  <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
  <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
  <![endif]-->
</head>
<body class="hold-transition skin-blue sidebar-mini">
	<div class="wrapper">

		<!-- Content Wrapper. Contains page content -->
		<div class="content-wrapper content-wrapper-error row">
			<!-- Main content -->
			<section class="content">
				<div class="error-page">
					<h2 class="headline text-red">500</h2>

					<div class="error-content">
						<h3><i class="fa fa-warning text-red"></i> Oops! Algo fue mal.</h3>

						<p>
							Estamos trabajando en solucionar este problema.
							Mientras puede ir al <a href="../../index.html">inicio</a> y volve a intentarlo.
						</p>
					</div>
				</div>
				<!-- /.error-page -->
			</section>
			<!-- /.content -->
            <!--======= ERROR SMG ========-->
            @if (HttpContext.Current.IsDebuggingEnabled)
            {
			<section class="error-msg col-sm-6 col-sm-offset-3">
				<div class="box box-default collapsed-box">
					<div class="box-header with-border">
						<h3 class="box-title">Error</h3>
						<div class="box-tools pull-right">
							<button type="button" class="btn btn-box-tool" data-widget="collapse"><i class="fa fa-plus"></i>
							</button>
						</div>
						<!-- /.box-tools -->
					</div>
					<!-- /.box-header -->
					<div class="box-body">
						<p>
							<b>Controller:</b> @ViewBag.controller<br />
							<b>Action:</b> @ViewBag.action <br />
                            <b>Exception:</b> @ViewBag.msg <br />
						</p>
				    </div>
				    <!-- /.box-body -->
			    </div>     
		    </section>
            }
	</div>
	<!-- /.content-wrapper -->
  <!-- Add the sidebar's background. This div must be placed
  immediately after the control sidebar -->
  <div class="control-sidebar-bg"></div>
</div>
<!-- ./wrapper -->

<!-- jQuery 2.2.3 -->
<script src="../../Plugins/jQuery/jquery-2.2.3.min.js"></script>
<!-- Bootstrap 3.3.6 -->
<script src="../../Plugins/bootstrap/js/bootstrap.min.js"></script>
<!-- FastClick -->
<script src="../../Plugins/fastclick/fastclick.js"></script>
<!-- AdminLTE App -->
<script src="../../Plugins/theme/app.min.js"></script>
<!-- AdminLTE for demo purposes -->
<script src="../../Plugins/theme/demo.js"></script>
</body>
</html>
