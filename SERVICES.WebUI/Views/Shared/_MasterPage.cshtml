
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title>Services - @ViewBag.Title</title>
    <!-- Tell the browser to be responsive to screen width -->
    <meta content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no" name="viewport">
    <!-- Bootstrap 3.3.6 -->
    <link rel="stylesheet" href="@Url.Content("~/Plugins/bootstrap/css/bootstrap.min.css")"/>
    <!-- Font Awesome -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.5.0/css/font-awesome.min.css">
    <!-- Ionicons -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/ionicons/2.0.1/css/ionicons.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="@Url.Content("~/Plugins/theme/AdminLTE.css")"/>
    <!-- AdminLTE Skins. Choose a skin from the css/skins
         folder instead of downloading all of them to reduce the load. -->
    <link rel="stylesheet" href="@Url.Content("~/Plugins/theme/skin-blue.css")" />

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    @RenderSection("head", false)
</head>
<body class="hold-transition sidebar-mini">
    <!-- Site wrapper -->
    <div class="wrapper">

        <!-- ==========  HEADER  ============= -->
        <header class="main-header">
            @{Html.RenderPartial("_Header");}
        </header>
        <!-- ========= END HEADER  ============ -->

        <!-- ==========  LEFT COLUMN  ============= -->
        <!-- Left side column. contains the sidebar -->
        <aside class="main-sidebar">
            <section class="sidebar">
                @{Html.RenderPartial("_SideMenu");}
            </section>
        </aside>
         <!-- =========END LEFT COLUMN ============= -->

        <!-- ==========  CONTENIDO  ============= -->
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            @RenderBody()
        </div>
        <!-- /.content-wrapper -->
        <!-- ========= END CONTENIDO ============ -->

        <!-- ==========  FOOTER  ============= -->
        <footer class="main-footer">
            @{Html.RenderPartial("_Footer");}
        </footer>
        <!-- ========= END FOOTER ============= -->

</div>
    <!-- ./wrapper -->
    <!-- ==========  SCRIPTS  ============= -->
    <!-- jQuery 2.2.3 -->
    <script src="@Url.Content("~/Plugins/jQuery/jquery-2.2.3.min.js")"></script>
    <!-- Bootstrap 3.3.6 -->
    <script src="@Url.Content("~/Plugins/bootstrap/js/bootstrap.min.js")"></script>
    <!-- SlimScroll -->
    <script src="@Url.Content("~/Plugins/slimScroll/jquery.slimscroll.min.js")"></script>
    <!-- FastClick -->
    <script src="@Url.Content("~/Plugins/fastclick/fastclick.js")"></script>
    <!-- AdminLTE App -->
    <script src="@Url.Content("~/Plugins/theme/app.min.js")"></script>
    <!-- AdminLTE for demo purposes -->
    <script src="@Url.Content("~/Plugins/theme/demo.js")"></script>
    @RenderSection("scripts", false)
    <!-- =========  END  SCRIPTS  ========= -->
</body>
</html>

